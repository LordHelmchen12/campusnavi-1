<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.fhl.campusnavi"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Start"
            android:label="@string/title_activity_start" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Map"
            android:label="@string/title_activity_map" >
        </activity>
        <activity
            android:name=".ShowAllPersons"
            android:label="@string/title_activity_show_all_persons" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="Start" />
        </activity>
        <activity
            android:name=".ShowAllPersonTyp"
            android:label="@string/title_activity_show_all_person_typ" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ShowAllPersons" />
        </activity>
        <activity
            android:name=".ShowPerson"
            android:label="@string/title_activity_show_person" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="de.fhl.campusnavi.ShowAllPersons" />
        </activity>
        <activity
            android:name=".ShowAllRaumTypen"
            android:label="@string/title_activity_show_all_raum_typen" >
        </activity>
        <activity
            android:name=".ShowAllRaeume"
            android:label="@string/title_activity_show_all_raeume" >
        </activity>
        <activity
            android:name=".ShowAllBedarfTypen"
            android:label="@string/title_activity_show_all_bedarf_typen" >
        </activity>
        <activity
            android:name=".ShowAllBedarf"
            android:label="@string/title_activity_show_all_bedarf" >
        </activity>
        <activity
            android:name=".ShowAllWcTypen"
            android:label="@string/title_activity_show_all_wc_typen" >
        </activity>
        <activity
            android:name=".ShowAllWc"
            android:label="@string/title_activity_show_all_wc" >
        </activity>
    </application>

</manifest>